<UserControl x:Class="PentagoAg.WinPhone.PentagoDot"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    FontFamily="{StaticResource PhoneFontFamilyNormal}"
    FontSize="{StaticResource PhoneFontSizeNormal}"
    Foreground="{StaticResource PhoneForegroundBrush}"
    >

    <UserControl.Resources>
        <SolidColorBrush x:Key="BlackDotFill" Color="#FF121111"/>
        <SolidColorBrush x:Key="WhiteDotFill"  Color="#FFDEDADA"/>
        <SolidColorBrush x:Key="EmptyDotFillNatural" Color="#FF85660F"/>
		<SolidColorBrush x:Key="EmptyDotFillRed" Color="#FF6C0E0E"/>
    </UserControl.Resources>

    <Ellipse x:Name="Elly">
        <Ellipse.StrokeThickness>5</Ellipse.StrokeThickness>
    </Ellipse>
</UserControl>
